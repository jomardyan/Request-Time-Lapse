{
  "manifest_version": 3,
  "name": "Request Time-Lapse",
  "version": "0.1.0",
  "description": "DevTools panel that records, groups, and replays network request history for API regression debugging.",
  "author": "Hayk Jomardyan",
  "homepage_url": "https://github.com/jomardyan/Request-Time-Lapse",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Request Time-Lapse - Click to view tutorial"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "tutorial.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
